{
  "name": "koa-handlebars-app",
  "version": "1.0.0",
  "description": "Performante Desktop App mit Koa.js & Handlebars",
  "main": "http://localhost:8080",
  "node-main": "server.js",
  "window": {
    "title": "Koa + Handlebars App",
    "width": 1200,
    "height": 800,
    "min_width": 800,
    "min_height": 600,
    "position": "center",
    "icon": "public/favicon.ico"
